    <script id="reviews-template" type="text/-x-handlebars-template">
        {{#each reviews}}
        <li class="booking-review-item">
            <div class="booking-review-item-header">
                <span class="booking-rating-score">{{average_score}}</span>
                <h3>
                    {{{headline}}}
                        <span class="booking-review-item-date">{{dateFormat date "MMMM YYYY"}}  by {{author.name}}</span>
                </h3>
            </div>
            <div class="booking-review-item-arguments">
            {{#if pros}}
                <div class="booking-review-item-argument booking-review-item-argument-pro">
                    <div class="booking-review-item-argument-header">
                        <span class="booking-review-item-icon">+</span>
                        <h4 class="booking-review-item-icon-title" title="Pros" aria-label="Pros"></h4>
                    </div>
                    <p>{{{pros}}}</p>
                </div>
            {{/if}}
            {{#if cons}}
                <div class="booking-review-item-argument">
                    <div class="booking-review-item-argument-header">
                        <span class="booking-review-item-icon">-</span>
                        <h4 class="booking-review-item-icon-title" title="Cons" aria-label="Cons"></h4>
                    </div>
                    <p>{{{cons}}}</p>
                </div>
            {{/if}}
            </div>
        </li>
        {{/each}}
    </script>

    <script id="modal-template" type="text/-x-handlebars-template" v-pre>
        <div class="modal">
            <div class="modal-wrap">
                <div class="modal-inner">
                    <button type="button" class="button button-clean modal-close">&times;</button>
                    <div class="modal-header">
                        <h1>{{title}}</h1>
                    </div>
                    <div class="modal-content">
                        {{{content}}}
                    </div>
                </div>
            </div>
            <div class="modal-bg"></div>
        </div>
    </script>

    <script id="room-images-template" type="text/-x-handlebars-template" v-pre>
        <div class="modal">
            <div class="modal-wrap">
                <div class="modal-inner">
                    <button type="button" class="button button-clean modal-close">&times;</button>
                    <div class="modal-header">
                        <h1>{{title}}</h1>
                    </div>
                    <div class="modal-content">
                        <div class="room-images-slider">
                            {{#each images}}
                            <div class="room-images-slide">
                                <div class="loader"></div>
                                <img data-src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" data-lazy="{{url}}">
                            </div>
                            {{/each}}
                        </div>
                        <div class="room-thumbs-slider" data-draggable>
                            <div class="room-thumbs-slider-inner">
                                {{#each images}}
                                <div class="room-thumbs-slide">
                                    <img data-index="{{@index}}" src="{{url}}">
                                </div>
                                {{/each}}
                            </div>
                        </div>
                        <div class="room-modal-details">
                            {{{content}}}
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-bg"></div>
        </div>
    </script>

        
